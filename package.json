{
  "name": "Lead-Email",
  "version": "1.0.0",
  "description": "Nominate you heros.",
  "scripts": {
    "start": "node dist",
    "prestart": "yarn build",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "mongo:seed": "babel-node node_modules/.bin/seed --data ./src/seeds --drop-collections",
    "dotenv": "babel-node node_modules/.bin/dotenv",
    "seed": "yarn dotenv yarn mongo:seed",
    "prettier": "prettier --write **/*.{js,json,yml,md}",
    "build": "babel src --copy-files --out-dir dist --source-maps",
    "start:dev": "cross-env NODE_ENV=dev nodemon --watch .env --watch src --exec babel-node src --source-maps",
    "test": "cross-env NODE_ENV=test mocha --exit --recursive --require @babel/register test",
    "test:coverage": "cross-env NODE_ENV=test nyc --require @babel/register --reporter lcov --reporter text mocha --exit --recursive test",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api"
  ],
  "private": true,
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@leapfrogtechnology/async-store": "^1.2.0",
    "@sentry/node": "^5.15.0",
    "app-module-path": "^2.2.0",
    "aws-sdk": "^2.693.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "dynamoose": "^2.3.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "mongo-seeding-cli": "^3.4.1",
    "mongodb": "^3.5.6",
    "mongoose": "^5.9.10",
    "mongoose-aggregate-paginate-v2": "^1.0.4",
    "mongoose-paginate-v2": "^1.3.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.15",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-dist": "^3.25.0",
    "uuid": "^8.3.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-leapfrog": "^2.0.1",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "12.9.0"
  }
}
